@model CreateHomeViewModel

<h2>Create</h2>

<form asp-action="create" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Home.Address"></label>
        <input class="form-control" asp-for="Home.Address" />
    </div>
    <div class="form-group">
        <label asp-for="Home.City"></label>
        <input class="form-control" asp-for="Home.City" />
    </div>
    <div class="form-group">
        <label asp-for="Home.State"></label>
        <input class="form-control" asp-for="Home.State" />
    </div>
    <div class="form-group">
        <label asp-for="Image"></label>
        <input type="file" accept=".jpg" class="form-control-file" asp-for="Image" />
    </div>
    <button type="submit" class="btn btn-primary">Create</button>
</form>

